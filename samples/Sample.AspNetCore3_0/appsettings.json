{
  "Unleash": {
    "UnleashApi": "http://localhost:4242/",
    "AppName": "Sample.AspNetCore2_1",
    "FetchTogglesInterval": "00:01:00",
    "SendMetricsInterval": "00:05:00",
    "Serialization": {
      "NewtonsoftJson": {
        "Encoding": "UTF8",
        "BufferSize": 65536
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}
